<window id="2901">
	<defaultcontrol>213</defaultcontrol>
	<animation effect="slide" start="-200,0" end="0,0" time="500">WindowOpen</animation>
	<animation effect="slide" start="0,0" end="-200,0" time="500">WindowClose</animation>
	<controls>
	
	<control type="image">
			<posx>-580</posx>
			<posy>170</posy>
			<height>400</height>
			<width>720</width>
			<texture>vispanel2.png</texture>
	</control>
	
	<control type="grouplist" id="213">
			<onclick>XBMC.PlayerControl(ShowVideoMenu)</onclick>
		<include>vismenugroup3</include>
		<onup>213</onup>
		<ondown>213</ondown>
		
		
					<control type="button" id="8">
				<label>Stop</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.PlayerControl(Stop)</onclick>
			</control>
		
			<control type="button" id="9">
				<label>Pause</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.PlayerControl(Play)</onclick>
				<visible>!Player.Paused</visible>
			</control>
			<control type="button" id="10">
				<label>Resume</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.PlayerControl(Play)</onclick>
				<visible>Player.Paused</visible>
				<onclick>XBMC.PlayerControl(ShowVideoMenu)</onclick>
			</control>
			<control type="button" id="11">
				<label>Rewind</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.PlayerControl(Rewind)</onclick>
			</control>
			<control type="button" id="12">
				<label>Forward</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.PlayerControl(Forward)</onclick>
			</control>
			<control type="button" id="13">
				<label>Video</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.ActivateWindow(123)</onclick>
			</control>
			<control type="button" id="199">
				<label>Audio</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.ActivateWindow(124)</onclick>
			</control>
			<control type="button" id="14">
				<label>Chapters</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.ActivateWindow(125)</onclick>
			</control>
			<control type="button" id="18">
				<label>Playlist</label>
				<include>contextformatbrowser3</include>
				<onclick>XBMC.ActivateWindow(28)</onclick>
			</control>
	</control>
	
	<control type="label">
	<posx>0</posx>
	<posy>528</posy>
		<textcolor>e1b1e021</textcolor>
		<label>OSD</label>
		<align>center</align>
		<width>120</width>
		<font>imdb</font>
	</control>

</controls>
</window>
